CREATE TABLE auth_user(
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    username CHAR(128),
    email CHAR(128) UNIQUE,
    first_name CHAR(512),
    last_name CHAR(512),
    password CHAR(512),
    registration_key CHAR(512),
    reset_password_key CHAR(512),
    registration_id CHAR(512)
);
